#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules/.pnpm/acorn@7.4.1/node_modules:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules/.pnpm/acorn@7.4.1/node_modules:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/routing-using-react-router-part-3/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/acorn" "$@"
else
  exec node  "$basedir/../../bin/acorn" "$@"
fi
